      <SOURCEOBJECTS>
     <SOURCEOBJECT DATASOURCENAME="Nucleus" DATAOBJECTNAME="USERS">
    
      <MAPPINGCOLUMNS>
        <MAPPINGCOLUMN DATACOLUMN="UserName" ATTRIBUTE ="UserName" />
        <MAPPINGCOLUMN DATACOLUMN="UserLogin" />
      </MAPPINGCOLUMNS>
       </SOURCEOBJECT>
      </SOURCEOBJECTS>
