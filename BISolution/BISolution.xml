  <DATASOURCES>
    <DATASOURCE NAME="Nucleus" TYPE="SQL Server">
      <SERVERNAME>KTHEJOKER\SQLEXPRESS</SERVERNAME>
      <DATABASENAME>Nucleus</DATABASENAME>
      <CONNECTIONSTRING></CONNECTIONSTRING>
      <DATAOBJECTS>
        <DATAOBJECT TYPE="DIMENSION" NAME="USERS">
          <SOURCEQUERY>select * from dbo.[USERS]</SOURCEQUERY>
          <MATCHDATASET>User</MATCHDATASET>
          <NATURALKEY>
            <NATURALKEYCOLUMN>UserName</NATURALKEYCOLUMN>
          </NATURALKEY>
          <LOADTYPE>FULL</LOADTYPE>
          <TIMESTAMPCOLUMN></TIMESTAMPCOLUMN>
        </DATAOBJECT>
        <DATAOBJECT TYPE="FACT" NAME="POWER_DEALS">
          <SOURCEQUERY>select * from Power_Deals</SOURCEQUERY>
          <MATCHDATASET>PowerDeal</MATCHDATASET>
          <NATURALKEY>
            <NATURALKEYCOLUMN>DealID</NATURALKEYCOLUMN>
          </NATURALKEY>
          <LOADTYPE>FULL</LOADTYPE>
          <TIMESTAMPCOLUMN></TIMESTAMPCOLUMN>
        </DATAOBJECT>
      </DATAOBJECTS>
    </DATASOURCE>
   </DATASOURCES>
   <CUBE>
    <DIMENSIONS>
      <DIMENSION NAME="USER">
        <ATTRIBUTES>
          <ATTRIBUTE TYPE="Name">UserName</ATTRIBUTE>
          <ATTRIBUTE TYPE="Name">UserLogin</ATTRIBUTE>
        </ATTRIBUTES>
        <HIERARCHIES></HIERARCHIES>
        <DATASET>User</DATASET>
